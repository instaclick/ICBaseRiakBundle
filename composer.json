{
    "name": "instaclick/base-riak-bundle",
    "description": "This bundle provides basic (riak) session support for Dating Engine",
    "keywords": ["base", "riak", "session"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "InstaClick Inc.",
            "homepage": "http://www.instaclick.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.dev:8080/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-riak": "~1.1",
        "symfony/symfony": "2.2.*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "IC\\Bundle\\Base\\RiakBundle": "" }
    },
    "autoload-dev": {
        "psr-4": { "IC\\Bundle\\Base\\TestBundle\\": "../TestBundle/" }
    },
    "target-dir": "IC/Bundle/Base/RiakBundle",
    "minimum-stability": "dev"
}
